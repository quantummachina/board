<div class="row-fluid">
	<div class="span12">
		===USERS===
		<table>
			<tr>
				<td>id</td>
				<td>name</td>
				<td>email</td>
				<td>suscription</td>
		<% for  u in User.reorder('created_at DESC').all  do  %>
			<tr>
				<td><%= u.id %></td>
				<td><%= u.name %></td>
				<td><%= u.email %></td>
				<td><%= time_ago_in_words(u.created_at) %></td>
			</tr>
		<% end %>
		</table>
	</div>
</div>