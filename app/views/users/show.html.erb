		<!--%= if current_user == @user then render 'collections/create_board' end %-->
		<hr>
		<h2>Owned</h2>
		<%= render 'shared/boards', boards:@boards %>
		<hr>
		<h2>Collaborating</h2>
		<%= render 'shared/boards', boards:@collabs %>
	</div>
</div>